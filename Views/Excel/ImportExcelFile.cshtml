
@{
    ViewData["Title"] = "ImportExcelFile";
}

<div class="container">
    <div class="row">
        <div class="col-md-6 offset-3">
            <form asp-action="ImportExcelFile" asp-controller="Excel" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label>Upload Excel File:</label>
                    <input type="file" name="FormFile" class="form-control" accept=".csv,.xls,.xlsx"/>
                </div>
                <input type="submit" class="btn btn-sm btn-primary" value="Upload"/>
            </form>
        </div>
    </div>
</div>

